    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mcollina/aedes-packet/issues"
  },
  "homepage": "https://github.com/mcollina/aedes-packet#readme",
  "dependencies": {},
  "devDependencies": {
    "faucet": "0.0.1",
    "license-checker": "^25.0.1",
    "nyc": "^14.1.1",
    "pre-commit": "^1.2.2",
    "snazzy": "^8.0.0",
    "standard": "^13.0.2",
    "tape": "^4.11.0"
  }
